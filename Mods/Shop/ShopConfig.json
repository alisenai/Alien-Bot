{
  "Mod Description": "Adds a configurable shop.",
  "Commands": {
    "Set Shop Command": {
      "Aliases": [
        "set-shop"
      ],
      "Help": "Sets the current channel as a shop with the provided name.",
      "Useage": [
        "set-shop {name}",
        "set-shop RolesShop"
      ],
      "Enabled": true
    },
    "List Shops Command": {
      "Aliases": [
        "list-shop"
      ],
      "Help": "Lists all existing shops.",
      "Useage": [
        "list-shop {page/none}",
        "list-shop 2",
        "list-shop"
      ],
      "Enabled": true
    },
        "Delete Shop Command": {
      "Aliases": [
        "delete-shop"
      ],
      "Help": "Deletes shop by name.",
      "Useage": [
        "delete-shop {name}",
        "delete-shop RolesShop"
      ],
      "Enabled": true
    }
  }
}