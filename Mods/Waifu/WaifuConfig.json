{
  "Mod Description": "Waifu > Laifu.",
  "Default Claim Amount": 500,
  "Claim Addition Amount": 50,
  "Commands": {
    "Divorce Waifu Command": {
      "Aliases": [
        "divorce"
      ],
      "Help": "Claims given user as your waifu.",
      "Useage": [
        "divorce {Waifu: Ping/Identifier}",
        "divorce @User",
        "divorce 123456789012345678"
      ],
      "Enabled": true
    },
    "Claim Waifu Command": {
      "Aliases": [
        "claim"
      ],
      "Help": "Claims given user as your waifu.",
      "Useage": [
        "claim {User: Ping/Identifier}",
        "claim @User",
        "claim 123456789012345678"
      ],
      "Enabled": true
    },
    "Waifu Info Command": {
      "Aliases": [
        "waifu-info"
      ],
      "Help": "Displays the waifu info of either the given user or the author.",
      "Useage": [
        "waifu-info {User: Empty/Ping/Identifier}",
        "waifu-info",
        "waifu-info @User",
        "waifu-info 123456789012345678"
      ],
      "Enabled": true
    }
  }
}